      ROUND( SAFE_CAST( SUM(
            CASE
              WHEN metric_name = 'in_ipv4_bytes' AND JSON_VALUE(labels, '$.group')= 'up-payload-ip-ni-if' THEN CAST(metric_increase_value AS FLOAT64)
              ELSE NULL
          END
            ) AS FLOAT64 ), 4 ) AS increase_in_ipv4_bytes_by_access,
