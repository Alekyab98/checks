                ELSE NULL
            END
              ) ) AS FLOAT64 ), 4 ) AS out_throughput,
            
      ROUND( SAFE_CAST( ( 8 * SUM (
              CASE
                WHEN metric_name = 'dl_ipv6_sent_bytes' THEN CAST(metric_rate_value AS FLOAT64)
                ELSE NULL
            END
              ) + 8 * SUM(
