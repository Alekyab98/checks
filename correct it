                ELSE NULL
            END
              ) ) AS FLOAT64 ), 4 ) AS in_throughput,



			  ROUND( SAFE_CAST( ( 8 * SUM(
              CASE
                WHEN metric_name = 'out_ipv4_bytes' AND JSON_VALUE(labels, '$.group') = 'up-payload-ip-ni-if' THEN CAST(metric_rate_value AS FLOAT64)
                ELSE NULL
            END
